{
  "platform": "github",
  "extends": [
    "config:base"
  ],
  "enabledManagers": ["regex"],
  "regexManagers": [
    {
      "fileMatch": [
        "variables.sh$"
      ],
      "matchStrings": [
        "GRPC_FULL_VERSION_BY_RELEASE=\\$\\{GRPC_FULL_VERSION_BY_RELEASE:-(?<currentValue>.*?)\\}\\n"
      ],
      "depNameTemplate": "grpc/grpc",
      "datasourceTemplate": "github-tags",
      "extractVersionTemplate": "^(?<version>\\d+\\.\\d+\\.\\d+(?:-pre\\d+)?)$"
    }
  ],
  {
      "matchPackageNames": ["grpc/grpc"],
      "allowedVersions": "/^\\d+\\.\\d+\\.\\d+$/"
  }
}
